extends templates/main
block variables
	- let pageTitle = TITLE_PART + ' | О компании';
	- let isHome = false; //- Если true то логотип не будет ссылкой
	- let currentPage = 2.01; // Для каждой страницы уникален. Номер смотреть в файле pug/templates/_vars.pug в массивах с пунктами меню
	- let currentSection = 2; // // Для каждой раздела уникален. Home -1, projects - 2, news - 3, pages - 4

block header
	include modules/header

block content
	article.about-page
		.about-page__inner.container
			h1.about-page__heading.heading.heading--large.__js_negative-shift О компании
			.about-page__text
				p Наша компания изготавливает светопрозрачные конструкции для всех типов зданий и сооружений: от элитных бизнес‑центров и крупных торгово‑развлекательных центров до типовых жилых домов.

				p Мы работаем с 2007 года и предлагаем нашим клиентам надёжные, проверенные решения, базирующиеся как на собственном, так и на мировом опыте быстро развивающейся высокотехнологичной строительной отрасли.

			section.about-page__facts
				h2.about-page__facts-heading.heading.__js_negative-shift Тысячи людей каждый день приходят в здания построенные с&nbsp;нашим участием

				ul.about-page__facts-list.facts.__js_number-parent
					li.facts__item
						.facts__item-num.__js_number.__js_negative-shift(data-end-value="196.5") 0
						.facts__item-text Общая площадь реализованных проектов
					li.facts__item
						.facts__item-num.__js_number.__js_negative-shift(data-end-value="481") 0
						.facts__item-text Реализованный проект за 13 лет деятельности
					li.facts__item
						.facts__item-num.__js_number.__js_negative-shift(data-end-value="86") 0
						.facts__item-text Сотрудников различных специальностей
					li.facts__item
						img(src="img/about/armchair.svg", width="41", height="47", alt="")
						a.facts__btn.btn(href= URLS.office) Офис

			section.about-page__growth
				h2.about-page__growth-heading.heading.__js_negative-shift Рост объемов
